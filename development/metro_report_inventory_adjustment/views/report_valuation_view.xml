<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="inventory_valuation_view" model="ir.ui.view">
        <field name="name">inventory.form.inherit</field>
        <field name="model">stock.inventory</field>
        <field name="inherit_id" ref="stock.view_inventory_form"/>
        <field name="arch" type="xml">
            <xpath expr="//header" position="inside">
                <button name="action_report" string="Print Report" type="object" class="oe_highlight" groups="stock.group_stock_user"/>
            </xpath>
        </field>
    </record>
</odoo>
