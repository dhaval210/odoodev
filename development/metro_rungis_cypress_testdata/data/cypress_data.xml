        <record id="cypress_location_1" model="stock.location">
            <field name="name">Cypress Test Location 1</field>
            <field name="barcode">cypress-loc-1</field>
            <field name="usage">internal</field>
            <field name="location_id" eval="41" />
        </record>
        <record id="cypress_location_2" model="stock.location">
            <field name="name">Cypress Test Location 2</field>
            <field name="barcode">cypress-loc-2</field>
            <field name="usage">internal</field>
